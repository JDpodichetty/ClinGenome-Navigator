# Security Keys
SECRET_KEY=your_secret_key_for_sessions_here

# External API Keys
# Add any other API keys your application needs
# EXTERNAL_API_KEY=your_external_api_key_here

# Application Configuration
ENVIRONMENT=development
DEBUG=false